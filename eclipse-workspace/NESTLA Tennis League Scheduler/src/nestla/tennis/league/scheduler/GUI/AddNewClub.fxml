<AnchorPane fx:id="rootPane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="375.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nestla.tennis.league.scheduler.GUI.AddNewClubController">
   <children>
      <Label alignment="CENTER" layoutX="188.0" layoutY="95.0" style="&#10;" text="Type Club Name" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="addNewClubClubName" layoutX="342.0" layoutY="96.0" promptText="Club A" />
      <Button fx:id="addNewClubAddClub" layoutX="356.0" layoutY="290.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Add Club" />
      <Button fx:id="addNewClubCancel" layoutX="210.0" layoutY="291.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Cancel" />
      <Label alignment="CENTER" layoutX="75.0" layoutY="139.0" style="&#10;" text="Select Total Number of Courts" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="addNewClubNumberOfCourts" layoutX="342.0" layoutY="140.0" prefWidth="150.0" promptText="Select Number" />
      <Label alignment="CENTER" layoutX="32.0" layoutY="179.0" style="&#10;" text="Select Number of Courts with lights" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
